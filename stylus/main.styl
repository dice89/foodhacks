@keyframes layer3 {
  0% {
    opacity 1
  }
  50% {
    opacity .3
  }
  100% {
    opacity 1
  }
}

#layer3 {
   animation: layer3 3.8s infinite;
}

@keyframes layer6 {
  0% {
    opacity 0.4
  }
  50% {
    opacity .75
  }
  100% {
    opacity 0.4
  }
}

#layer4 {
   animation: layer4 4.2s infinite;
}

@keyframes layer4 {
  0% {
    opacity .3
  }
  50% {
    opacity 1
  }
  100% {
    opacity .3
  }
}

#layer5 {
   animation: layer5 3.5s infinite;
}

@keyframes layer5 {
  0% {
    opacity 1
  }
  66% {
    opacity .3
  }
  100% {
    opacity 1
  }
}
#layer6 {
   animation: layer6 11s infinite;
}




