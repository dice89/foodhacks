// Timeline

.tmtimeline
  margin 80px auto
  padding 0 0 0 50px
  list-style none
  position relative
  width 100%
    
  &:before
    content ''
    position absolute
    top 0
    bottom 0
    width 8px
    background #0A0B16
    left 88px
    +below(500px)
      left 40px
  & > li
    position relative
    &:nth-child(odd)
      .tmlabel
        background #0A0B16
        &:after
          border-right-color #0A0B16
    .tmlabel
      margin 0 0 15px 120px
      background #0A0B16
      color #fff
      padding 2em
      position relative
      border-radius 5px
      +below(500px)
        margin 0 0 15px 62px
      h6
        margin-top 0px
        margin-bottom 0px
        font-size 19px
        color rgba(255,255,255,0.85)
        font-family: 'TWS Savory', Helvetica-Neue, sans-serif;
        font-weight: 300;
      &:after
        border solid transparent
        content " "
        height 0
        width 0
        position absolute
        pointer-events none
        border-right-color #0A0B16
        border-width 10px
        top 10px
        transform translate(-52px,0)
      p
        line-height 140%
    .tmday
      background #0A0B16
      width 180px
      height 180px
      border-radius 50%
      display flex
      align-items center
      justify-content center
      transform translate(-47px,-47px)
      h3
        font-size 30px
        font-family 'Quicksand'
      +below(500px)
        transform translate(-52px,-47px)
        border-radius 40px
        height 80px
        width 115%
        margin-top 80px
        box-sizing border-box
        padding 15px
        h3
          font-size 45px
      

    .tmicon
      width 85px
      height 85px
      -webkit-font-smoothing antialiased
      position absolute
      color #fff
      background #0A0B16
      border-radius 50%
      box-shadow 0 0 0 2px #0A0B16
      text-align center
      top 0
      margin -20px 0 0 0
      display flex
      justify-content center
      align-items center
      transition 0.2s ease-in
      +below(500px)
        transform translate(-47px,0)
      &:hover
        opacity 0.7
        cursor pointer

